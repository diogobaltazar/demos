{
    "editor.fontSize": 12,
    "editor.detectIndentation": false,
    "window.zoomLevel": 0,
    "git.autofetch": true,
    "git.confirmSync": false,
    "python.linting.enabled": false,
    "python.venvPath": "/usr/local/lib/python2.7/dist-packages/pyspark/jars",
    "databricks.connections": [
        {
            "apiRootUrl": "https://westeurope.azuredatabricks.net",
            "cloudProvider": "Azure",
            "displayName": "workspace.commercial.maersk",
            "localSyncFolder": "~/sync_folder.databricks_workspace"
        }
    ],
    "databricks.connection.default.exportFormats": 
