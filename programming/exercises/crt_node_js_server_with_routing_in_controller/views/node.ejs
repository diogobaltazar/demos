<!DOCTYPE html>
<html lang="en" >
<head>
  <meta charset="UTF-8">
  <title>new <%= ids %></title>
</head>
    <body>
        <%- include('partials/nav.ejs') %> 
        <ul>
            <% ids.forEach(id => { %>
                <li><%= id %></li>
            <% }) %>
        </ul>

        <form id='node-form' method='POST' action='/new'>
            <label for='node_id'>id</label>
            <input type='text' name='node_id' value="">
            
            <label for='node_nature'>nature</label>
            <input type='text' name='node_nature' value="">
            
            <label for='node_type'>type</label>
            <input type='text' name='node_type' value="">
            
            <input type='submit' value="new">
        </form>
        
    </body>
</html>
